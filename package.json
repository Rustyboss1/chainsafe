{
  "name": "dapp-boilerplate",
  "version": "1.0.0",
  "description": "A dapp boilerplate for standardising development",
  "main": "index.js",
  "repository": "https://github.com/ChainSafe/dapp-boilerplate.git",
  "author": "Ryan Noble <ryanjnoble@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "yarn compile",
    "clean": "rm -rf ./node_modules && rm -rf ./*/**/node_modules",
    "start": "yarn compile && craco --max_old_space_size=4096 start",
    "build": "craco --max_old_space_size=4096 build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "lingui-version": "lingui --version",
    "lint": "eslint './{src}/**/*.{js,jsx,ts,tsx}'"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=1"
  },
  "dependencies": {
    "@babel/core": "7.14.6",
    "@babel/runtime": "7.14.6",
    "@chainsafe/browser-storage-hooks": "1.0.1",
    "@chainsafe/common-components": "1.0.29",
    "@chainsafe/common-theme": "1.0.11",
    "@chainsafe/web3-context": "1.2.0",
    "@lingui/core": "3.10.4",
    "@lingui/react": "3.10.2",
    "@types/uuid": "8.3.1",
    "axios": "0.21.1",
    "babel-loader": "8.1.0",
    "babel-plugin-macros": "2.8.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "bnc-onboard": "1.27.0",
    "clsx": "1.1.1",
    "dayjs": "1.10.6",
    "formik": "2.2.9",
    "mime-matcher": "1.0.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-scripts": "3.4.4",
    "react-toast-notifications": "2.5.1",
    "react-zoom-pan-pinch": "1.6.1",
    "typescript": "4.0.8",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@craco/craco": "5.9.0",
    "@lingui/babel-preset-react": "2.9.2",
    "@lingui/cli": "3.10.2",
    "@lingui/macro": "3.7.2",
    "@types/jest": "26.0.24",
    "@types/node": "16.3.2",
    "@types/react": "17.0.47",
    "@types/react-beforeunload": "2.1.1",
    "@types/react-dom": "17.0.17",
    "@types/react-pdf": "5.0.5",
    "@types/react-toast-notifications": "2.4.1",
    "@types/yup": "0.29.13",
    "@types/zxcvbn": "4.4.1",
    "babel-plugin-macros": "2.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
